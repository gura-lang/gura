#!/usr/bin/env gura
import(markdown)
import(uuid)

text = R'''
# uuid Module

The `uuid` module provides functions to generate UUIDs.
To utilize it, import the `uuid` module using `import` function.
'''

write(doc:markdown.document) = {
	doc << text
	doc << R'''
	## Module Functions
	'''
	doc << [
		uuid.generate
	]
	doc
}

if (__name__ == '__main__') {
	write(markdown.document()).render@html(sys.stdout)
}
