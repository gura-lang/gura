#!/usr/bin/env gura
import(markdown)
import(uuid)

write(doc:markdown.document) = {
	doc << R'''
	# uuid Module
	## Abstract
	## Sample
	'''
	doc << R'''
	## Module Functions
	'''
	doc << [
		uuid.generate
	]
	doc
}

if (__name__ == '__main__') {
	write(markdown.document()).render@html(sys.stdout)
}
