#!/usr/bin/env gura
import(markdown)

write(doc:markdown.document) = {
	doc << R'''
	## os Module
	### Abstract
	### Sample
	'''
	doc << '### Module Functions'
	doc << [
		os.clock
		os.exec
		os.fromnative
		os.getenv
		os.putenv
		os.redirect
		os.sleep
		os.symlink
		os.tonative
		os.unsetenv
	]
	doc
}

if (__name__ == '__main__') {
	write(markdown.document()).render@html(sys.stdout)
}
