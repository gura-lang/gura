#!/usr/bin/env gura
import(markdown)

text = R'''
# os Module

The `os` module provides functions that are specific to each OS environment.
'''

write(doc:markdown.document) = {
	doc << text
	doc << R'''
	## Module Functions
	'''
	doc << [
		os.clock
		os.exec
		os.fromnative
		os.getenv
		os.putenv
		os.redirect
		os.sleep
		os.symlink
		os.tonative
		os.unsetenv
	]
	doc
}

if (__name__ == '__main__') {
	write(markdown.document()).render@html(sys.stdout)
}
