#!/usr/bin/env gura
import(moddoc)

text@en = R'''
# os Module

The `os` module provides functions that are specific to each OS environment.
This is a built-in module, so you can use it without being imported.

## Module Function

${moddoc.reference(`en
os.clock
os.exec
os.fromnative
os.getenv
os.putenv
os.redirect
os.sleep
os.symlink
os.tonative
os.unsetenv
)}
'''.embed()

moddoc.register(locals(), %{`en => text@en})
