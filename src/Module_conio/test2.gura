#!/usr/bin/env gura
import(conio)

txt = chr(0x30 + [0..9].round(64)).join()
for (region in [`up, `down, `line]) {
	conio.clear()
	printf('%s %d\n', txt, 0..19)
	[x, y] = [0, 5]
	conio.moveto(x, y)
	print('#')
	conio.moveto(x, y)
	sys.stdin.readline()
	conio.moveto(x, y)
	conio.clear(region)
	sys.stdin.readline()
}
for (region in [`left, `right, `line]) {
	conio.clear()
	printf('%s %d\n', txt, 0..19)
	[x, y] = [20, 5]
	conio.moveto(x, y)
	print('#')
	conio.moveto(x, y)
	sys.stdin.readline()
	conio.moveto(x, y)
	conio.clear(region)
	sys.stdin.readline()
}
