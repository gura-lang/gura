#!/usr/bin/env gura
import(markdown)
import(hash)

write(doc:markdown.document) = {
	doc << R'''
	## hash Module
	### Abstract
	### Sample
	'''
	doc << '### Module Functions'
	doc << [
		hash.md5
		hash.sha1
		hash.crc32
	]
	doc << '### hash.accumulator Class'
	doc << [
		hash.accumulator.init
		hash.accumulator.update
	]
	doc
}

if (__name__ == '__main__') {
	write(markdown.document()).render@html(sys.stdout)
}
