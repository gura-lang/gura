#!/usr/bin/env gura
import(markdown)

text = R'''
# fs Module

The `fs` module provices measures to access and modify information in file systems.

'''

write(doc:markdown.document) = {
	doc << text
	doc << R'''
	## Module Functions
	'''
	doc << [
		fs.chdir
		fs.chmod
		fs.copy
		fs.cpdir
		fs.getcwd
		fs.mkdir
		fs.remove
		fs.rename
		fs.rmdir
	]
	doc
}

if (__name__ == '__main__') {
	write(markdown.document()).render@html(sys.stdout)
}
