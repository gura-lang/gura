#!/bin/sh
# This script may likely run with the right of the super user.
# The option --no-local-dir avoids creation of the local directory with his ownership.
program-gura/gura --no-local-dir -I @GURA_MODULE_SRCDIR@ @PROJECT_SOURCE_DIR@/build-modules.gura -I @GURA_INCLUDE_SRCDIR@ -M @GURA_MODULE_SRCDIR@ build-install $*
