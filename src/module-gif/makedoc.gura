#!/usr/bin/env gura
import(markdown)
import(gif)

write(doc:markdown.document) = {
	doc << R'''
	## gif Module
	### Abstract
	### Sample
	'''
	doc << '### gif.content Class'
	doc << [
		gif.content
		gif.content.addimage
		gif.content.write
	]
	doc << '### Extension of image Class'
	doc << [
		image.gifread
		image.gifwrite
	]
	doc
}

if (__name__ == '__main__') {
	write(markdown.document()).render@html(sys.stdout)
}
