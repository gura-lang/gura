#!/usr/bin/env gura
import(markdown)
import(gif)

write(doc:markdown.document) = {
	doc << R'''
	# gif Module
	## Abstract
	## Sample
	'''
	doc << R'''
	## gif.content Class
	'''
	doc << [
		gif.content
		gif.content.addimage
		gif.content.write
	]
	doc << R'''
	## Extension of image Class
	'''
	doc << [
		image.read@gif
		image.write@gif
	]
	doc
}

if (__name__ == '__main__') {
	write(markdown.document()).render@html(sys.stdout)
}
