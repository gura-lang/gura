#!/usr/bin/env gura
import(markdown)
import(yaml)

text = R'''
# yaml Module

The `yaml` module provides measures to read/write YAML files.
'''

write(doc:markdown.document) = {
	doc << text
	doc << R'''
	## Module Functions
	'''
	doc << [
		yaml.compose
		yaml.parse
		yaml.read
		yaml.write
	]
	doc
}

if (__name__ == '__main__') {
	write(markdown.document()).render@html(sys.stdout)
}
