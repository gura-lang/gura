#!/usr/bin/env gura
import(markdown)
import(yaml)

write(doc:markdown.document) = {
	doc << R'''
	# yaml Module
	## Abstract
	## Sample
	'''
	doc << R'''
	## Module Functions
	'''
	doc << [
		yaml.compose
		yaml.parse
		yaml.read
		yaml.write
	]
	doc
}

if (__name__ == '__main__') {
	write(markdown.document()).render@html(sys.stdout)
}
