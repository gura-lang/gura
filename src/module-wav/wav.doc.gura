#!/usr/bin/env gura
import(moddoc)
import(wav)

text@en = R'''
# wav Module

## Module Function

## Extension to audio Class

This module extends the `audio` class with methods described here.

${moddoc.reference(`en
audio.read@wav
audio.write@wav
)}
'''.embed()

moddoc.register(locals(), %{`en => text@en})
