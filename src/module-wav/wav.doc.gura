#!/usr/bin/env gura
import(markdown)
import(wav)

text@en = R'''
# wav Module

## Module Function

## Extension to audio Class

This module extends the `audio` class with methods described here.

${markdown.makedoc@function(`en
audio.read@wav
audio.write@wav
)}
'''.embed()

markdown.makedoc@module(locals(), %{`en => text@en})
