#!/usr/bin/env gura
import(markdown)
import(wav)

text = R'''
# wav Module

## Module Functions

## Extension to audio Class

This module extends the `audio` class with methods described here.

${markdown.funcdoc(
audio.read@wav
audio.write@wav
)}
'''.template().render()

write(doc:markdown.document, lang:symbol) = {
	doc << text
	doc
}

if (__name__ == '__main__') {
	write(markdown.document(), `en).render@html(sys.stdout)
}
