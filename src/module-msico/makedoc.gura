#!/usr/bin/env gura
import(markdown)
import(msico)

text = R'''
# msico Module
## Abstract
## Sample
'''

write(doc:markdown.document) = {
	doc << text
	doc << R'''
	## Module Functions
	'''
	doc << [
	]
	doc << R'''
	## msico.content Class
	'''
	doc << [
		msico.content
		msico.content.write
		msico.content.addimage
	]
	doc << R'''
	## Extension of image Class
	'''
	doc << [
		image.read@msico
	]
	doc
}

if (__name__ == '__main__') {
	write(markdown.document()).render@html(sys.stdout)
}
