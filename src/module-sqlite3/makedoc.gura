#!/usr/bin/env gura
import(markdown)
import(sqlite3)

write(doc:markdown.document) = {
	doc << R'''
	## sqlite3 Module
	### Abstract
	### Sample
	'''
	doc << '### Module Functions'
	doc << [
	]
	doc << '### sqlite3.db Class'
	doc << [
		sqlite3.db
		sqlite3.db.close
		sqlite3.db.exec
		sqlite3.db.getcolnames
		sqlite3.db.query
		sqlite3.db.transaction
	]
	doc
}

if (__name__ == '__main__') {
	write(markdown.document()).render@html(sys.stdout)
}
