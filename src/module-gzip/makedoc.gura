#!/usr/bin/env gura
import(markdown)
import(gzip)

text = R'''
# gzip Module
## Abstract
## Sample
'''

write(doc:markdown.document) = {
	doc << text
	doc << R'''
	## Module Functions
	'''
	doc << [
		gzip.reader
		gzip.writer
	]
	doc << R'''
	## Extension of image Class
	'''
	doc << [
		stream.reader@gzip
		stream.writer@gzip
	]
	doc
}

if (__name__ == '__main__') {
	write(markdown.document()).render@html(sys.stdout)
}
