#!/usr/bin/env gura
import(markdown)
import(tar)

write(doc:markdown.document) = {
	doc << R'''
	# tar Module
	## Abstract
	## Sample
	'''
	doc << R'''
	## tar.reader Class
	'''
	doc << [
		tar.reader
		tar.reader.entries
	]
	doc << R'''
	## tar.writer Class
	'''
	doc << [
		tar.writer
		tar.writer.add
		tar.writer.close
	]
	doc
}

if (__name__ == '__main__') {
	write(markdown.document()).render@html(sys.stdout)
}
