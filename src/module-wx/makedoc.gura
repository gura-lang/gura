#!/usr/bin/env gura
import(markdown)

text = R'''
# wx Module

The `wx` module provides functions and methods of wxWidgets library.
'''

write(doc:markdown.document) = {
	doc << text
	doc << '### Module Functions'
	doc << [
	]
	doc
}

if (__name__ == '__main__') {
	write(markdown.document()).render@html(sys.stdout)
}
