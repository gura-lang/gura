#!/usr/bin/env gura
import(markdown)
import(bzip2)

text = R'''
# bzip2 Module


'''

write(doc:markdown.document) = {
	doc << text
	doc << R'''
	## Module Functions
	'''
	doc << [
		bzip2.reader
		bzip2.writer
	]
	doc << R'''
	## Methods Added to stream Class
	'''
	doc << [
		stream.reader@bzip2
		stream.writer@bzip2
	]
	doc
}

if (__name__ == '__main__') {
	write(markdown.document()).render@html(sys.stdout)
}
