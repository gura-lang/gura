#!/usr/bin/env gura
import(markdown)
import(bzip2)

write(doc:markdown.document) = {
	doc << R'''
	## bzip2 Module
	### Abstract
	### Sample
	'''
	doc << '### Module Functions'
	doc << [
		bzip2.reader
		bzip2.writer
	]
	doc << '### Extension of image Class'
	doc << [
		stream.reader@bzip2
		stream.writer@bzip2
	]
	doc
}

if (__name__ == '__main__') {
	write(markdown.document()).render@html(sys.stdout)
}
