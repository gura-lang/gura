#!/usr/bin/env gura
import(markdown)

write(doc:markdown.document) = {
	doc << R'''
	## markdown Module
	'''
	doc << '### Module Functions'
	doc << [
		markdown.document
		markdown.setpresenter
	]
	doc << '### markdown.document Class'
	doc << [
		markdown.document.parse
		markdown.document.read
	]
	doc << '### markdown.item Class'
	doc << [
		markdown.item.print
	]
	doc
}

if (__name__ == '__main__') {
	write(markdown.document()).render@html(sys.stdout)
}
