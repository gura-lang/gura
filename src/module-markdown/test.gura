#!/usr/bin/env gura
import(markdown)

str = R'''
line-before

<@div>
code here

    code-line1
    code-line2
    code-line3

code end
</@div>

line-after
'''

str = R'''
line-before

<@div>
# title1

## title1-1

## title1-2

# title2

# title3

</@div>

line-after
'''

doc = markdown.document()
doc << str
doc.root.print()
