#!/usr/bin/env gura
import(markdown)

str = R'''
<table>
<tr><th>Property</th><th>Type</th><th>R/W</th><th>Note</th></tr>

<tr><td><code>access_capability</code></td><td><code>symbol</code></td><td>R</td>
<@td>Indicates what access is permitted to the storage by following symbols:

- `` `ReadWrite`` .. Reading and writing are permitted.
- `` `ReadOnly`` .. Reading only is permitted.
- `` `ReadOnlyWithObjectDeletion`` .. Reading and deleting objects are permitted.</@td></tr>
<tr><td><code>filesystem_type</code></td><td><code>symbol</code></td><td>R</td>
<@td>Indicates the type of the file system by following symbols:

- `` `Undefined``
- `` `GenericFlat``
- `` `GenericHierarchical``
- `` `DCF``</@td></tr>

</table>
'''

doc = markdown.document()
doc << str
//doc.root.print()
doc.render@html()
