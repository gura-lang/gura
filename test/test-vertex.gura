#!/usr/bin/env gura
import(helper.test) {*}

testcase('constructor') {
	tester(`vertex(30, 40))
	tester(`vertex(30, 40, 50))
	tester(`vertex(30, 40, 50, 8))
}
testcase('operator') {
	v1 = vertex(30, 40, 50)
	v2 = vertex(5, 6, 7)
	tester(`v1)
	tester(`v2)
	tester(`(-v1))
	tester(`(v1 + v2))
	tester(`(v1 - v2))
	tester(`(v1 / 4))
	tester(`(v1 * 4))
}
testcase('vertex.tolist()') {
	v = vertex(30, 40, 50, 8)
	tester(`v.tolist())
	tester(`v.tolist():w)
}
